{
  "name": "nyt_archive_browser",
  "version": "1.0.0",
  "description": "Browse archived NYT articles by month.",
  "engines": {
    "node": "10.16.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "cross-env NODE_ENV=development webpack --mode development & cross-env NODE_ENV=development node server/index.js",
    "build": "cross-env NODE_ENV=production webpack -p --mode production",
    "lint:scss": "csscomb src/scss && stylelint src/scss --fix"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "author": "Tobias Wei√ü",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "csscomb": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
