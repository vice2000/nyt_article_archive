{
  "name": "nyt_archive_browser",
  "version": "1.0.0",
  "description": "Browse archived NYT articles by month.",
  "engines": {
    "node": "10.16.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "cross-env NODE_ENV=development webpack --mode development & cross-env NODE_ENV=development node server/index.js",
    "build": "cross-env NODE_ENV=production webpack -p --mode production",
    "lint:scss": "csscomb src/scss && stylelint src/scss --fix"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "author": "Tobias Wei√ü",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.4.10",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "localforage": "^1.7.3",
    "lodash": "^4.17.13",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "csscomb": "^4.2.0",
    "eslint": "^5.15.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
